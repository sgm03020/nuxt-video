{
  "name": "nuxt-video",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean:dist": "rimraf dist",
    "dev": "cross-env NODE_ENV=development HOST=0.0.0.0 PORT=3000 nuxt-ts",
    "dev2": "cross-env NODE_ENV=production HOST=0.0.0.0 PORT=3000 nuxt-ts",
    "dev3": "cross-env NODE_ENV=development HOST=0.0.0.0 PORT=3000 nuxt",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "start2": "cross-env NODE_ENV=production HOST=0.0.0.0 PORT=3000 nuxt start",
    "generate": "nuxt-ts generate",
    "generate1": "cross-env NODE_ENV=development nuxt-ts generate",
    "generate2": "cross-env NODE_ENV=production nuxt-ts generate",
    "modern": "cross-env NODE_ENV=production nuxt generate --modern",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxt/content": "^1.15.0",
    "@nuxt/http": "^0.6.4",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxt/typescript-runtime": "^2.1.0",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/color-mode": "^2.1.1",
    "@nuxtjs/markdownit": "^2.0.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "cross-env": "^7.0.3",
    "graphql": "^16.0.1",
    "graphql-tag": "^2.12.6",
    "nuxt": "^2.15.8",
    "nuxt-ssr-screen-size": "^1.0.0",
    "vue-cli-plugin-vuetify-preset-rally": "^1.0.4",
    "vue-cli-plugin-vuetify-preset-reply": "^1.0.4",
    "vue-lazy-youtube-video": "^2.3.0",
    "vue-slick-carousel": "^1.0.6",
    "vue-youtube": "^1.4.0",
    "vuetify": "^2.5.14"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/vuetify": "^1.12.1",
    "@types/node": "^16.11.7",
    "prettier": "^2.4.1",
    "stylelint": "^14.0.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-config-standard-scss": "^2.0.1",
    "stylelint-scss": "^4.0.0"
  }
}
